.section-block-added-csv
  display flex
  align-items center
.section-csv--wrap
  display flex

.section-csv-green
  display flex
  justify-content center
  align-items center
  background-color #09B983
  box-shadow: 0px 0px 20px rgba(9, 185, 131, 0.2)
  border-radius 30px
  font-family 'Montserrat 500',sans-serif
  font-size 12px
  color #fff
  height 35px
  width max-content
  padding 12px 20px
  margin-right 10px
  cursor pointer

.section-csv-yellow
  display flex
  justify-content center
  align-items center
  background-color #2F80EC
  box-shadow: 0px 0px 20px rgba(253,191,90,0.2);
  border-radius 30px
  font-family 'Montserrat 500',sans-serif
  font-size 12px
  color #fff
  height 35px
  width max-content
  padding 12px 20px
  margin-right 15px
  cursor pointer

